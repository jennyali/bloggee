{
  "name": "bloggee",
  "version": "0.0.1",
  "main": "server/index.js",
  "repository": "https://github.com/jennyali/bloggee.git",
  "author": "Jenny Ali <jenny_pc@hotmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:server": "node server/index.js",
    "start:client": "cd client && yarn start",
    "storybook": "cd client && yarn storybook",
    "migrate:make": "node -r ./server/env.js ./node_modules/.bin/knex --knexfile ./server/knexfile.js migrate:make",
    "migrate:latest": "node -r ./server/env.js ./node_modules/.bin/knex --knexfile ./server/knexfile.js migrate:latest",
    "migrate:rollback": "node -r ./server/env.js ./node_modules/.bin/knex --knexfile ./server/knexfile.js migrate:rollback",
    "seed:make": "node -r ./server/env.js ./node_modules/.bin/knex --knexfile ./server/knexfile.js seed:make",
    "seed:run": "node -r ./server/env.js ./node_modules/.bin/knex --knexfile ./server/knexfile.js seed:run"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "connect-session-knex": "^2.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "http-errors": "^1.7.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.18",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "objection": "^2.2.14",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "qs": "^6.9.4",
    "slugify": "^1.5.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "faker": "^5.5.3",
    "http-terminator": "^3.0.0",
    "mongodb-memory-server": "^6.6.1",
    "prettier": "^1.19.1",
    "rosie": "^2.0.1",
    "supertest": "^4.0.2"
  }
}
